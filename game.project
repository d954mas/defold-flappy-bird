[project]
title = Flappy Zombie
version = 1.01
dependencies = 

[bootstrap]
main_collection = /main.collectionc
render = /render/my.renderc

[input]
game_binding = /game.input_bindingc

[display]
width = 480
height = 720
high_dpi = 1

[physics]
scale = 0.2
debug = 0

[script]
shared_state = 1

[library]
include_dirs = https://github.com/d954mas/stageUI/archive/master.zip,  https://github.com/britzl/defold-googleanalytics/archive/master.zip https://github.com/britzl/defcon/archive/master.zip  https://github.com/britzl/defnet/archive/2.1.zip,https://github.com/d954mas/router/archive/master.zip

[graphics]
default_texture_min_filter = nearest
default_texture_mag_filter = nearest
texture_profiles = /my.texture_profiles

[android]
app_icon_48x48 = /assets/android/mipmap-mdpi/ic_launcher.png
app_icon_72x72 = /assets/android/mipmap-hdpi/ic_launcher.png
app_icon_96x96 = /assets/android/mipmap-xhdpi/ic_launcher.png
app_icon_144x144 = /assets/android/mipmap-xxhdpi/ic_launcher.png
app_icon_192x192 = /assets/android/mipmap-xxxhdpi/ic_launcher.png
package = com.d954mas.flappybird
version_code = 2

