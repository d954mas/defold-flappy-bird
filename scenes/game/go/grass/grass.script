local start_x = 0
local end_x = 0

local function animate()
	go.set(".","position.x",start_x)
	go.animate(".","position.x",go.PLAYBACK_ONCE_FORWARD,end_x,go.EASING_LINEAR,10,0,animate)
end

function init(self)
	start_x = go.get_position().x
	local width = -165
	end_x = start_x + width
	animate()
end
